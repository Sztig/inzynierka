{% extends 'base.html.twig' %}

{% block body %}
    <a class="btn btn-outline-primary" href="{{ app.request.headers.get('referer') }}">Go back</a><br><br>
    {% for user in users %}
            <a class="btn btn-outline-primary" href="{{ path('stamp_user', {'id': user.id}) }}"><i class="fa fa-user"></i>  {{ user.username }}</a>
    {% endfor %}
{% endblock %}