{% extends 'base.html.twig' %}

{% block body %}
    {% if stamps|length < 1 %}

        <p>Your news page is empty</p>

    {% else %}
        <h6 class="border-bottom border-gray pb-2 mb-0">Recent updates</h6>


        {% for stamp in stamps %}
            {{ include('feed/feedRaw.html.twig', { 'stamp': stamp}) }}
        {% endfor %}
    {% endif %}
{% endblock %}